{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "fixtures/(.*)$": "<rootDir>/../fixtures/$1",
    "@lib/(.*)$": "<rootDir>/../libs/package-template/src/$1",
    "@lib": "<rootDir>/../libs/package-template/src"
  }
}
